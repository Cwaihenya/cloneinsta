<% if logged_in? %>
<h1><%= @user.name %></h1>
<p>

  <%= image_tag @user.image.url %>
</p>

<% if current_user.id == @user.id %>
    <%= link_to 'Favorites List', favorites_path %> |
    <%= link_to 'Edit profile', edit_user_path(@user) %>
  <% end %>
<% else %>
